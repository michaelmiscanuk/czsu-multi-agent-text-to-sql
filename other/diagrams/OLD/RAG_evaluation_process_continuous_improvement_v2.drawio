        </mxCell>
        <mxCell id="step1_desc" value="&lt;div&gt;• Consider what to evaluate&lt;/div&gt;&lt;div&gt;• What constitutes a &quot;good&quot; result&lt;/div&gt;&lt;div&gt;• Align with intended use case&lt;/div&gt;&lt;div&gt;• Define quality standards&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=#f3e5f5;align=left;verticalAlign=top;whiteSpace=wrap;rounded=1;fontSize=10;fontFamily=Helvetica;fontColor=#4a148c;spacingLeft=8;fontStyle=0;spacingTop=4;" parent="1" vertex="1">
          <mxGeometry x="88" y="165" width="204" height="65" as="geometry" />
        </mxCell>
        <mxCell id="step2_box" value="&lt;b&gt;2. DEFINE GOLDEN DATASET&lt;/b&gt;" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#b2dfdb;strokeColor=#00796B;fontSize=12;fontStyle=1;gradientColor=#80CBC4;align=center;verticalAlign=top;spacingTop=4;fontFamily=Helvetica;fontColor=default;spacing=2;" parent="1" vertex="1">
          <mxGeometry x="340" y="140" width="220" height="100" as="geometry" />
        </mxCell>
        <mxCell id="step2_desc" value="&lt;div&gt;• Input examples + optional outputs&lt;/div&gt;&lt;div&gt;• Good coverage, edge cases&lt;/div&gt;&lt;div&gt;• Outputs verified against sources&lt;/div&gt;&lt;div&gt;• Aligned with application scope&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=#E0F2F1;align=left;verticalAlign=top;whiteSpace=wrap;rounded=1;fontSize=10;fontFamily=Helvetica;fontColor=#004D40;spacingLeft=8;fontStyle=0;spacingTop=4;" parent="1" vertex="1">
          <mxGeometry x="348" y="165" width="204" height="68" as="geometry" />
        </mxCell>
        <mxCell id="step3_box" value="&lt;b&gt;3. DEFINE TASK&lt;/b&gt;" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff3e0;strokeColor=#e65100;fontSize=12;fontStyle=1;gradientColor=#FFDAB3;align=center;verticalAlign=top;spacingTop=4;fontFamily=Helvetica;fontColor=default;spacing=2;" parent="1" vertex="1">
          <mxGeometry x="600" y="140" width="220" height="100" as="geometry" />
        </mxCell>
        <mxCell id="step3_desc" value="&lt;div&gt;• Core element to evaluate&lt;/div&gt;&lt;div&gt;• Entire app or part of it&lt;/div&gt;&lt;div&gt;• Function: inputs → outputs&lt;/div&gt;&lt;div&gt;• Target of assessment&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=#FFF3E0;align=left;verticalAlign=top;whiteSpace=wrap;rounded=1;fontSize=10;fontFamily=Helvetica;fontColor=#E65100;spacingLeft=8;fontStyle=0;spacingTop=4;" parent="1" vertex="1">
          <mxGeometry x="608" y="165" width="204" height="68" as="geometry" />
        </mxCell>
        <mxCell id="step4_box" value="&lt;b&gt;4. DEFINE METRIC&lt;/b&gt;" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#c8e6c9;strokeColor=#388E3C;fontSize=12;fontStyle=1;gradientColor=#A5D6A7;align=center;verticalAlign=top;spacingTop=4;fontFamily=Helvetica;fontColor=default;spacing=2;" parent="1" vertex="1">
          <mxGeometry x="860" y="140" width="220" height="100" as="geometry" />
        </mxCell>
        <mxCell id="step4_desc" value="&lt;div&gt;• Quantitative quality measure&lt;/div&gt;&lt;div&gt;• Relevant to specific task&lt;/div&gt;&lt;div&gt;• Aligned with end-user needs&lt;/div&gt;&lt;div&gt;• Guides architectural decisions&lt;/div&gt;" style="text;html=1;strokeColor=none;fillColor=#E8F5E9;align=left;verticalAlign=top;whiteSpace=wrap;rounded=1;fontSize=10;fontFamily=Helvetica;fontColor=#1B5E20;spacingLeft=8;fontStyle=0;spacingTop=4;" parent="1" vertex="1">

        <mxCell id="notes_box" value="&lt;b&gt;Key Points from Evaluation Theory:&lt;/b&gt;" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#f5f5f5;strokeColor=#666666;fontSize=11;fontStyle=1;align=left;verticalAlign=top;spacingTop=8;spacingLeft=10;" parent="1" vertex="1">
